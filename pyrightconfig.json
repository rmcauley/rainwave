{
  "stubPath": "src/typestubs",
  "include": ["src/**/*.py"],
  "exclude": ["**/__pycache__", "src/frontend", "**/node_modules", "**/.*", ".venv"],
  "typeCheckingMode": "strict",
  "strict": ["src/**/*.py"],
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": ["src"]
    }
  ],
  "reportMissingModuleSource": "none"
}
